{
  "name": "qanuni",
  "version": "1.0.0",
  "description": "Qanuni - Legal ERP System for Law Firms",
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron": "electron .",
    "dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "api": "nodemon server/api-server.js",
    "dev:web": "concurrently \"npm run api\" \"npm run dev\""
  },
  "dependencies": {
    "date-fns": "^3.3.1",
    "electron-is-dev": "^2.0.0",
    "lucide-react": "^0.330.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sql.js": "^1.10.0",
    "xlsx": "^0.18.5",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "electron": "^28.2.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "wait-on": "^7.2.0",
    "nodemon": "^3.0.2"
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  }
}
